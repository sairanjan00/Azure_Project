{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "canada-music-factory"
		},
		"DataLake Storage link Parameterized_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'DataLake Storage link Parameterized'"
		},
		"Kakudi DB Sql Source_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Kakudi DB Sql Source'"
		},
		"DataLake Storage link Parameterized_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://canada7.dfs.core.windows.net/"
		},
		"Kakudi DB Sql Source_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "canada-music-server.database.windows.net"
		},
		"Kakudi DB Sql Source_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "kakudi_db"
		},
		"Kakudi DB Sql Source_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "jhatia"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "canada-music-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"DataLake Storage link Parameterized_accountKey": {
						"value": "[parameters('DataLake Storage link Parameterized_accountKey')]"
					},
					"Kakudi DB Sql Source_password": {
						"value": "[parameters('Kakudi DB Sql Source_password')]"
					},
					"DataLake Storage link Parameterized_properties_typeProperties_url": {
						"value": "[parameters('DataLake Storage link Parameterized_properties_typeProperties_url')]"
					},
					"Kakudi DB Sql Source_properties_typeProperties_server": {
						"value": "[parameters('Kakudi DB Sql Source_properties_typeProperties_server')]"
					},
					"Kakudi DB Sql Source_properties_typeProperties_database": {
						"value": "[parameters('Kakudi DB Sql Source_properties_typeProperties_database')]"
					},
					"Kakudi DB Sql Source_properties_typeProperties_userName": {
						"value": "[parameters('Kakudi DB Sql Source_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}